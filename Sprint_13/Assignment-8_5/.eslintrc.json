{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": "eslint:recommended",
    "parserOptions": {
        "ecmaVersion": "latest"
    },
    "rules": {
        "indent" : ["error",2],
        "quotes":  ["error","single"],
        "no-unused-vars" : "error",
        "arrow-spacing":["error", 2],
        "brace-style":["error","alman"]

    }
}
